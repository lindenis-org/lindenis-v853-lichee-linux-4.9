				allwinner,function = "io_disabled";
				allwinner,muxsel = <0xf>;
				allwinner,drive = <1>;
				allwinner,pull = <1>;
			};

			sdc2_pins_a: sdc2@0 {
				allwinner,pins = "PC1", "PC4", "PC5", "PC6",
						 "PC7", "PC8", "PC9",
						 "PC11", "PC2", "PC3", "PC0";
				allwinner,function = "sdc2";
				allwinner,muxsel = <3>;
				allwinner,drive = <2>;
				allwinner,pull = <1>;
			};

			sdc2_pins_b: sdc2@1 {
				allwinner,pins = "PC1", "PC4", "PC5", "PC6",
						 "PC7", "PC8", "PC9",
						 "PC11", "PC2", "PC3", "PC10",
						 "PC0";
				allwinner,function = "io_disabled";
				allwinner,muxsel = <0xf>;
				allwinner,drive = <1>;
				allwinner,pull = <1>;
			};

			sdc2_pins_c: sdc2@2 {
				allwinner,pins = "PC10";
				allwinner,function = "sdc2";
				allwinner,muxsel = <3>;
				allwinner,drive = <2>;
				allwinner,pull = <2>;
			};

			csi_mclk0_pins_a: csi_mclk0@0 {
				allwinner,pins = "PA12";
				allwinner,pname = "mipi_csi_mclk0";
				allwinner,function = "mipi_csi_mclk0";
				allwinner,muxsel = <4>;
